def factorial(n: int): int {
  int result = 0;
  if (n < 2) {
    result = 1;
  } else {
    result = n * factorial(n - 1);
  }
  return result;
}

def main(): int {
  int n = input('enter a number: ');
  println();
  print('fact(');
  print(n);
  print(') = ');
  print(factorial(n));
  return 0;
}
